{
  "name": "vue-mpa",
  "version": "0.0.18",
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev:sit",
    "dev:sit": "node --loader ts-node/esm build/dev.server.js sit",
    "dev:pre": "node --loader ts-node/esm build/dev.server.js pre",
    "dev:prd": "node --loader ts-node/esm build/dev.server.js prd",
    "build": "cross-env NODE_ENV=production npm run build:sit",
    "build:sit": "cross-env NODE_ENV=production node --loader ts-node/esm build/build.ts sit",
    "build:pre": "cross-env NODE_ENV=production node --loader ts-node/esm build/build.js pre",
    "build:prd": "cross-env NODE_ENV=production node --loader ts-node/esm build/build.js prd",
    "lint": "eslint --ext .js,.vue,.ts src && stylelint --ext",
    "lint:fix": "eslint --fix --ext .js,.vue,.ts src",
    "stylelint": "stylelint 'src/**/*.{vue,less}'",
    "stylelint:fix": "stylelint 'src/**/*.{vue,less}' --fix",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "keywords": [
    "vue3",
    "webpack5",
    "mpa",
    "esm"
  ],
  "author": "caiyong",
  "license": "ISC",
